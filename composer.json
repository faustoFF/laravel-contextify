{
    "name": "faustoff/laravel-contextify",
    "description": "Contextual logging with inline notifications for Laravel.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Faustoff\\Contextify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faustoff\\Contextify\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "faustoFF",
            "email": "kulakov89@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/log": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/notifications": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "monolog/monolog": "^2.0|^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel-notification-channels/telegram": "^2.0|^3.0",
        "phpunit/phpunit": "^9.6 || ^10.0 || ^11.0",
        "orchestra/testbench": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Faustoff\\Contextify\\ContextifyServiceProvider"
            ]
        }
    },
    "scripts": {
        "cs": "php-cs-fixer fix --dry-run --diff",
        "cs:fix": "php-cs-fixer fix",
        "test": "phpunit --testdox"
    },
    "suggest": {
        "laravel-notification-channels/telegram": "Allows sending notifications to Telegram"
    },
    "config": {
        "audit": {
            "ignore": {
                "PKSA-8qx3-n5y5-vvnd": "Support for old laravel versions"
            }
        }
    }
}
